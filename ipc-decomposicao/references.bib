@article{brazAnaliseProcessoEliminacao2018,
  title = {{An\'alise do processo de elimina\c{c}\~ao da transmiss\~ao da mal\'aria na Amaz\^onia brasileira com abordagem espacial da varia\c{c}\~ao da incid\^encia da doen\c{c}a em 2016}},
