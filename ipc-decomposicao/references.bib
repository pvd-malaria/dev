@article{feged-rivadeneiraEthnographyParasiteQuantitative2019,
  title = {Ethnography of a Parasite: {{A}} Quantitative Ethnographic Observation of Forest Malaria in the {{Amazon}} Basin},
  shorttitle = {Ethnography of a Parasite},
  author = {{Feged-Rivadeneira}, Alejandro and Evans, Sian},
  year = {2019},
  month = dec,
  journal = {Scandinavian Journal of Public Health},
  volume = {47},
  number = {8},
  pages = {820--831},
  publisher = {{SAGE Publications Ltd STM}},
  issn = {1403-4948},
  doi = {10.1177/1403494818756561},
  abstract = {Aims: Malaria in the Amazon basin is persistently more prevalent among low density populations (1\textendash 4 people/km2km2{$<$}math display="inline" id="math1-1403494818756561" overflow="scroll" altimg="eq-00001.gif"{$><$}mrow{$><$}mi{$>$}k{$<$}/mi{$><$}msup{$><$}mi{$>$}m{$<$}/mi{$><$}mn{$>$}2{$<$}/mn{$><$}/msup{$><$}/mrow{$><$}/math{$>$}). Describing malaria transmission in small populations, such as ethnic minorities in the Amazon basin, living in reserves in groups that amount to 110\textendash 450 individuals, is fundamental for the implementation of adequate interventions. Here, we examine malaria transmission in a context of high prevalence in a small population of N\"ukak ethnicity (ethnic group n=400-650n=400-650{$<$}math display="inline" id="math2-1403494818756561" overflow="scroll" altimg="eq-00002.gif"{$><$}mrow{$><$}mi{$>$}n{$<$}/mi{$><$}mo{$>$}={$<$}/mo{$><$}mn{$>$}400{$<$}/mn{$><$}mo{$>-<$}/mo{$><$}mn{$>$}650{$<$}/mn{$><$}/mrow{$><$}/math{$>$} individuals, study group, n=108n=108{$<$}math display="inline" id="math3-1403494818756561" overflow="scroll" altimg="eq-00003.gif"{$><$}mrow{$><$}mi{$>$}n{$<$}/mi{$><$}mo{$>$}={$<$}/mo{$><$}mn{$>$}108{$<$}/mn{$><$}/mrow{$><$}/math{$>$} individuals) living in the peri-urban area of a city with 35,00035,000{$<$}math display="inline" id="math4-1403494818756561" overflow="scroll" altimg="eq-00004.gif"{$><$}mrow{$><$}mn{$>$}35{$<$}/mn{$><$}mo{$>$},{$<$}/mo{$><$}mspace width="0.25em"{$><$}/mspace{$><$}mn{$>$}000{$<$}/mn{$><$}/mrow{$><$}/math{$>$} inhabitants in the Amazon basin. Methods: Using methods from behavioral ecology, we conducted a quantitative ethnography and collected data to inform of individual behavioral profiles. Individual malarial infection reports were available from the local public health offices, so each behavioral profile was associated with an epidemic profile for the past 5 years. Results: Our research shows that, in-line with current opinion, malaria among the N\"ukak is not associated with an occupational hazard risk and follows a holoendemic pattern, where children are most susceptible to the parasite. Parasite loads of malarial infection among the N\"ukak persist at much higher rates than in any other neighboring ethnicity, which indicates an association between high incidence rates and endemicity. Conclusions: We hypothesize that malarial infection in the forest follows a pattern where the parasite persists in pockets of holoendemicity, and occupational hazard risk for individuals outside those pockets is associated with behaviors that take place in the proximity of the pockets of endemicity.},
  language = {en},
  keywords = {Amazon,anthropology,behavioral ecology,demography,disease ecology,epidemiology,hunter-gatherer,malaria,Quantitative ethnography,vector-borne disease},
  file = {C\:\\Users\\vinic\\Zotero\\storage\\FAFX9SIY\\Feged-Rivadeneira e Evans - 2019 - Ethnography of a parasite A quantitative ethnogra.pdf}
}

